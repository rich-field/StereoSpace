!!!
%html
  %head
    %title StereoSpace
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true
    = javascript_include_tag 'application', 'data-turbolinks-track' => true
    = csrf_meta_tags
  %body

    %nav
      %div#logo Logo goes here
      %ul
        %li= link_to("new song", "", :target => '_blank')
        %li#add-timeline= link_to("Add timeline", "#")
        %li#play= link_to("Play", "#")
        %li#record= link_to("record", "#")
        %li#rewind= link_to("Rewind", "#")
        %li#hide= link_to("hide timelines", "#")
    = yield